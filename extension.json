{
	"name": "SendGrid",
	"version": "3.0",
	"author": [
		"Derick Alangi"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:SendGrid",
	"descriptionmsg": "sendgrid-desc",
	"license-name": "GPL-2.0-or-later",
	"requires": {
		"MediaWiki": ">= 1.34.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\SendGrid\\": "includes/"
	},
	"MessagesDirs": {
